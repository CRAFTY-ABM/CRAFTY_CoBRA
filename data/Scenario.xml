<scenario startTick="2016" endTick="2100" world="EU"
		version="${@(Runs.csv, Version)}"
		regionalisation = "${@(Runs.csv, Regionalisation)}"
		scenario="${@(Runs.csv, Scenario)}"
		>
	<capitals class="org.volante.abm.serialization.CSVCapitalLoader" file="csv/Capitals.csv" indexed="true"/>
	<services class="org.volante.abm.serialization.CSVServiceLoader" file="csv/Services.csv" indexed="true"/>
	<outputs outputDirectory="output/%v/%w/%k/%s/%i/%r" filePattern="%s-%i-%r-%o" tickPattern="%s-%i-%r-%o-%y" >
		<output class="org.volante.abm.output.CellTable" perRegion="true" addRegion="false" addCellRegion="false"
        	addServices="true" addCapitals="true" addCompetitiveness="true" addLandUseIndex="true" addLandUse="true" addAgent="true" 
        	doubleFormat="0.000" startYear="2016" everyNYears="1"/> 
    	<output class="org.volante.abm.output.AggregateAFTCompositionCSVOutputter" outputSums="true" perRegion="true" startYear="2000" everyNYears="1"/>
		<output class="org.volante.abm.output.AggregateAFTCompetitivenessCSVOutputter" perRegion="true" startYear="2016" everyNYears="1"/>
		<output class="org.volante.abm.output.AggregateDemandSupplyCSVOutputter" perRegion="true" startYear="2016" everyNYears="1"/>
				<output class="org.volante.abm.output.TakeoverCellOutputter" perRegion="true" everyNYears="1" filePerTick="false" addRegion="false"/>
		<output class="org.volante.abm.output.GivingInStatisticsOutputter" startYear="2016" perRegion="true" everyNYears="1" filePerTick="false" addRegion="false"/>
		<output class="org.volante.abm.output.ActionCSVOutputter" agentpattern=".*Inst" filePerTick="false" perRegion="true" startYear="2016" everyNYears="1"/>
	</outputs>

<!-- 	<displays  class="org.volante.abm.visualisation.DefaultModelDisplays">
		<display class="org.volante.abm.visualisation.CapitalDisplay" initial="Crop.productivity" title="Capitals"/>
		<display class="org.volante.abm.visualisation.ProductionDisplay" initial="Meat" title="Production"/>
		<display class="org.volante.abm.comi.visualisation.TestAgents" title="AgentTypes"/>
		<display class="org.volante.abm.visualisation.CompetitivenessDisplay" initial="IA" title="Competitiveness"/>
		<display class="org.volante.abm.visualisation.SubmodelDisplays" title="Submodels"/>
	</displays>		-->

	<displays class="org.volante.abm.visualisation.NoModelDisplays"/>
			
	<globalBtReposFile>./institutions/global/GlobalInstitutionsBtRepos.xml</globalBtReposFile>
	<globalInstitutionFile>${@(Runs.csv, GlobalInstitutions_xml)}</globalInstitutionFile>
	<worldLoaderFile>xml/World_CSV.xml</worldLoaderFile> 
</scenario>